<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.plan.demo.user.dao.UserManagerMapper" >

  <select id="findDriverMaxId" resultType="java.lang.Long">
    select max(id) as id
    from tb_driver
  </select>
  <select id="findPassengerMaxId" resultType="java.lang.Long">
    select max(id) as id
    from tb_passenger
  </select>
  
</mapper>